{
  "redis" : {
    "host": "localhost",
    "port": 6379
  },
  "services": [
    {
      "service_identifier": "auth",
      "host": "localhost",
      "port": 8080,
      "routes": [
        {
          "gateway_path": "/register",
          "service_path": "/register"
        },
        {
          "gateway_path": "/authenticate",
          "service_path": "/authenticate"
        }
      ]
    },
    {
      "service_identifier": "car",
      "host": "localhost",
      "port": 8082,
      "routes": [
        {
          "gateway_path": "/api/car/create",
          "service_path": "/v1.CarService/Create"
        },
        {
          "gateway_path": "/api/car/find",
          "service_path": "/v1.CarService/Find"
        }
      ]
    }
  ]
}
